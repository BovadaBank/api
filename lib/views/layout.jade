block vars
  - var title

doctype html
html(lang='en')
  head
    block title
      title=title ? (siteName + ' - ' + title) : siteName
    block css
      link(href='https://maxcdn.bootstrapcdn.com/bootswatch/3.3.5/paper/bootstrap.min.css', rel='stylesheet')
  body
    include navbar
    div.container
      block content